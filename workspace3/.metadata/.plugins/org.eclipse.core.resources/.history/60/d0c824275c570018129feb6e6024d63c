package com.basic;

import java.io.IOException;
import java.io.PrintWriter;
import java.util.Enumeration;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

public class FourthServlet extends HttpServlet{
		protected void doGet(HttpServletRequest request, HttpServletResponse response) throws IOException, ServletException
		{
			/*HttpSession session = request.getSession(true);
			if(session ==  null)
			{
				System.out.println("No session available for client");
			}*/
	        
			response.setContentType("text/html");
			
			PrintWriter out = response.getWriter();
			
			out.println("<html>");
			out.println("<body>");
			
			String fname =null;
			String lname =null;
			String state =null;
			String city =null;
			String phone =null;
			String email =null;
			
			fname = request.getParameter("fname");
			lname = request.getParameter("lname");
			state = request.getParameter("state");
			city = request.getParameter("city");
			phone = request.getParameter("phone");
			email = request.getParameter("email");
			
			out.println("<form action='display3' method='get'>");
			
			
			out.println("First Name:<input type='text' value="+fname+" name='fname'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("Last Name: <input type='text' value="+lname+"name='lname'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("City Name:<input type='text' value="+state+" name='city'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("State Name:<input type='text' value="+city+" name='state'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("Phone Name:<input type='text' readonly value="+phone+" name='phone'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("Email Name:<input type='text' readonly value="+email+" name='email'/>");
			out.println("<br>");
			out.println("<br>");
			out.println("<input type='submit' value='Submit' name='continue'/>");
			
			out.println("</form>");
			out.println("</body>");
			out.println("</html>");
		}
}
